{
  "name": "prestaalba/ps_webhooks",
  "description": "Trigger real-time notifications on PrestaShop events attaching object data in JSON format to the request.",
  "authors": [
    {
      "name": "Experto PrestaShop",
      "homepage": "https://www.youtube.com/@ExpertoPrestaShop"
    }
  ],
  "autoload": {
    "classmap": [
      "classes"
    ]
  },
  "config": {
    "prepend-autoloader": false,
    "platform": {
      "php": "5.6.1"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@optimize-autoload -o --no-dev --ignore-platform-reqs"
    ],
    "optimize-autoload": "@composer dump-autoload -o --no-dev"
  },
  "license": "AFL-3.0",
  "type": "prestashop-module"
}